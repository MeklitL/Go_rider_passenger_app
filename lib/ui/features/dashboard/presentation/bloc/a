shareContest(BuildContext context) async {
    log.w('Preparing to generate link to share');
    String generatedLink = await DynamicLinkHelper()
        .generateDynamicLink(
      pathName: 'get-contest',
      id: state.contestFeed!.contest!.contestId!,
      title: state.contestFeed!.contest!.title,
      description: state.contestFeed!.contest!.description,
      imageUrl: state.contestFeed!.contest!.contestCoverImageData!.imageName!,
    )
        .then((link) {
      Share.share(
        link,
        sharePositionOrigin: Rect.fromLTWH(
          0,
          0,
          screenSize(context).width,
          screenSize(context).height / 2,
        ),
      );
      log.wtf('Generated link for contest: $link');
      return link;
    });
  }